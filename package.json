{
  "name": "terminal-snippet",
  "displayName": "terminalSnippet",
  "description": "terminalSnippet is a VSCode extension that allows you to create and manage terminal snippets.",
  "version": "1.0.1",
  "publisher": "VS-Modules",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2201009-kikukawa/terminal-snippet-extension.git"
  },
  "engines": {
    "vscode": "^1.93.1"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "views": {
      "terminalSnippet-id": [
        {
          "type": "webview",
          "id": "terminalSnippet-id",
          "name": "View"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "terminalSnippet-id",
          "title": "terminalSnippet",
          "icon": "./resources/codicon--send.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "comment-js": "--- JSのビルド（既存のスクリプトをリネーム） ---",
    "build:js": "node ./esbuild.js",
    "watch:js": "node ./esbuild.js --watch",
    "comment-css": "--- CSSのビルド（Tailwind用） ---",
    "build:css": "postcss ./src/webview/styles.css -o ./out/styles.css",
    "watch:css": "postcss ./src/webview/styles.css -o ./out/styles.css --watch",
    "comment-main": "--- 統合ビルド ---",
    "compile": "npm-run-all build:js build:css",
    "watch": "npm-run-all -p watch:css watch:js",
    "lint": "eslint src --ext ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.93.1",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "autoprefixer": "^10.4.22",
    "esbuild": "^0.16.10",
    "esbuild-plugin-copy": "^2.0.1",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vscode/codicons": "^0.0.40",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/2201009-kikukawa/terminal-snippet-extension/issues"
  },
  "homepage": "https://github.com/2201009-kikukawa/terminal-snippet-extension#readme"
}
